{
  "name": "dishonored2-calculator",
  "version": "1.0.0",
  "description": "Power calculator for Dishonored 2.",
  "author": "Anthony Hastings <ar.hastings@gmail.com> (https://antwan1986.github.io/)",
  "license": "MIT",
  "private": true,
  "repository": "github:anthonyhastings/dishonored2-power-calculator",
  "main": "server/index.mts",
  "scripts": {
    "build": "yarn run webpack:production",
    "browserslist:debug": "browserslist --config=.browserslistrc",
    "browserslist:update": "npx browserslist@latest --update-db && yarn list caniuse-lite",
    "eslint:base": "eslint --ext .js,.ts,.tsx,.mts --max-warnings 0",
    "eslint:check": "eslint-config-prettier src/components/app/avatar/index.ts",
    "preexpress:build": "rimraf dist-server/",
    "express:build": "tsc --project ./server/tsconfig.json",
    "express:dev-server": "nodemon server/index.mts",
    "express:production": "node dist-server/index.mjs",
    "lint": "time npm-run-all --parallel --print-label --continue-on-error lint:*",
    "lint:css": "time stylelint src/**/*.scss",
    "lint:js": "time yarn run eslint:base server/ src/ support/ webpack/ jest.config.ts",
    "start": "yarn run express:production",
    "test": "jest",
    "types:check": "npx tsc --noEmit --project tsconfig.json && npx tsc --noEmit --project server/tsconfig.json",
    "webpack:dev-server": "webpack serve --config=webpack/env.development.ts",
    "webpack:production": "webpack --config=webpack/env.production.ts",
    "webpack:stats": "yarn run webpack:production --env stats"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,mts}": "yarn run eslint:base",
    "*.scss": "stylelint"
  },
  "dependencies": {
    "@babel/runtime": "7.16.7",
    "@reduxjs/toolkit": "1.5.0",
    "axios": "0.21.2",
    "classnames": "2.3.1",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-winston": "4.0.5",
    "lodash": "4.17.21",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "6.2.1",
    "regenerator-runtime": "0.13.5",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/plugin-transform-runtime": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.0.2",
    "@commitlint/config-conventional": "16.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@types/compression": "1.7.0",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/css-minimizer-webpack-plugin": "^3.0.0",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.170",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/node": "14.18.5",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.16",
    "@types/react-test-renderer": "17.0.1",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "babel-jest": "27.4.6",
    "babel-loader": "8.2.2",
    "copy-webpack-plugin": "8.1.1",
    "css-loader": "5.2.4",
    "css-minimizer-webpack-plugin": "3.0.0",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "25.3.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "4.2.5",
    "image-minimizer-webpack-plugin": "2.2.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "9.0.0",
    "jest": "27.4.7",
    "lint-staged": "12.1.7",
    "mini-css-extract-plugin": "2.4.6",
    "nodemon": "2.0.15",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.5",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "7.2.0",
    "prettier": "2.5.1",
    "react-refresh": "0.11.0",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "sass": "1.47.0",
    "sass-loader": "12.4.0",
    "semantic-release": "18.0.1",
    "snapshot-diff": "0.9.0",
    "stylelint": "14.2.0",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-order": "5.0.0",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.6.0-dev.20220105",
    "webpack": "5.65.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.2",
    "webpack-merge": "5.8.0"
  }
}
