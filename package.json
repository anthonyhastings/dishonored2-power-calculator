{
  "name": "dishonored2-calculator",
  "version": "1.0.0",
  "description": "Power calculator for Dishonored 2.",
  "author": "Anthony Hastings <ar.hastings@gmail.com> (https://antwan1986.github.io/)",
  "license": "MIT",
  "private": true,
  "main": "server/index.mjs",
  "scripts": {
    "build": "yarn run webpack:production",
    "eslint-check": "eslint --print-config src/components/app/container/index.js | eslint-config-prettier-check",
    "lint": "yarn run lint:css && yarn run lint:js",
    "lint:css": "stylelint src/**/*.scss",
    "lint:js": "eslint --ext js --ext mjs server/ src/ support/ webpack/ jest.config.mjs postcss.config.js",
    "pretest": "yarn run lint",
    "start": "node --experimental-json-modules server/index.mjs",
    "test": "jest",
    "webpack:dev-server": "webpack-dev-server --config=webpack/env.development.js",
    "webpack:production": "webpack --config=webpack/env.production.js",
    "webpack:stats": "yarn run webpack:production -- --env.stats=true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,mjs}": "eslint",
    "*.scss": "stylelint"
  },
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "cors": "2.8.5",
    "express": "4.17.1",
    "immutable": "4.0.0-rc.12",
    "lodash": "4.17.15",
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "4.12.21",
    "react-immutable-proptypes": "2.1.0",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.5",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@hot-loader/react-dom": "16.13.0",
    "autoprefixer": "9.7.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.5.3",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jest": "23.10.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "imagemin-webpack-plugin": "2.4.2",
    "jest": "26.0.1",
    "lint-staged": "10.2.2",
    "mini-css-extract-plugin": "0.9.0",
    "moxios": "0.4.0",
    "node-sass": "4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "redux-mock-store": "1.5.4",
    "sass-loader": "8.0.2",
    "stylelint": "13.3.3",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0",
    "stylelint-scss": "3.17.2",
    "terser-webpack-plugin": "3.0.1",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  }
}
