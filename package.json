{
  "name": "dishonored2-calculator",
  "version": "1.0.0",
  "description": "Power calculator for Dishonored 2.",
  "author": "Anthony Hastings <ar.hastings@gmail.com> (https://antwan1986.github.io/)",
  "license": "MIT",
  "private": true,
  "repository": "github:anthonyhastings/dishonored2-power-calculator",
  "main": "server/index.mts",
  "type": "module",
  "scripts": {
    "build": "time npm-run-all --parallel --print-label client:build server:build",
    "client:build": "vite build",
    "client:dev": "vite dev",
    "client:stats": "VITE_STATS=true vite build",
    "eslint:base": "eslint --ext .ts,.tsx,.mts --max-warnings 0",
    "eslint:check": "eslint-config-prettier src/components/app/avatar/index.ts",
    "lint": "time npm-run-all --parallel --print-label --continue-on-error lint:*",
    "lint:css": "time stylelint src/**/*.scss",
    "lint:js": "time yarn run eslint:base server/ src/ support/ jest.config.ts vite.config.ts",
    "prepare": "husky install",
    "server:build": "rimraf dist-server/ && tsc --project ./server/tsconfig.json",
    "server:dev": "nodemon --exec ts-node --esm server/index.mts",
    "server:start": "node dist-server/index.mjs",
    "test": "jest",
    "types:check": "tsc --noEmit --project tsconfig.json && tsc --noEmit --project server/tsconfig.json",
    "start": "yarn run server:start"
  },
  "lint-staged": {
    "*.{ts,tsx,mts}": "yarn run eslint:base",
    "*.scss": "stylelint"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.7.1",
    "axios": "0.24.0",
    "classnames": "2.3.1",
    "compression": "1.7.4",
    "dotenv": "10.0.0",
    "express": "4.17.2",
    "express-winston": "4.2.0",
    "lodash": "4.17.21",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.1",
    "winston": "3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.2",
    "@commitlint/config-conventional": "16.0.0",
    "@types/compression": "1.7.2",
    "@types/jest": "29.0.3",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.62",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.21",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "25.3.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "jest": "29.0.3",
    "lint-staged": "12.1.7",
    "msw": "0.39.2",
    "nodemon": "2.0.15",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.5",
    "postcss-preset-env": "7.7.1",
    "prettier": "2.5.1",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "rollup-plugin-visualizer": "5.8.1",
    "sass": "1.47.0",
    "semantic-release": "19.0.3",
    "snapshot-diff": "0.9.0",
    "stylelint": "14.2.0",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-order": "5.0.0",
    "ts-jest": "29.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.7.2",
    "vite": "3.1.3",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-imagemin": "0.6.1"
  }
}
