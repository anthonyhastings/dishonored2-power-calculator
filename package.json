{
  "name": "dishonored2-calculator",
  "version": "1.0.0",
  "description": "Power calculator for Dishonored 2.",
  "author": "Anthony Hastings <ar.hastings@gmail.com> (https://antwan1986.github.io/)",
  "license": "MIT",
  "private": true,
  "main": "scripts/index.js",
  "scripts": {
    "build": "npm run webpack:production",
    "lint": "eslint --ext .js server/ src/ webpack/",
    "pretest": "npm run lint",
    "start": "node server/index.js",
    "test": "jest",
    "test:upload-coverage": "npm test && cat jest-coverage/lcov.info | coveralls",
    "webpack:dev-server": "webpack-dev-server --env.target=development --config=webpack/webpack.config.js",
    "webpack:production": "webpack --env.target=production --config=webpack/webpack.config.js --colors --progress",
    "webpack:stats": "webpack --env.target=production --env.stats=true --config=webpack/webpack.config.js --colors --progress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
