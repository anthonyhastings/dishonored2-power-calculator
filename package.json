{
  "name": "dishonored2-calculator",
  "version": "1.0.0",
  "description": "Power calculator for Dishonored 2.",
  "author": "Anthony Hastings <ar.hastings@gmail.com> (https://antwan1986.github.io/)",
  "license": "MIT",
  "private": true,
  "main": "server/index.mjs",
  "scripts": {
    "build": "yarn run webpack:production",
    "browserslist:debug": "browserslist --config=.browserslistrc",
    "browserslist:update": "npx browserslist@latest --update-db && yarn list caniuse-lite",
    "eslint:base": "eslint --ext .js,.mjs,.ts,.tsx --max-warnings 0",
    "eslint:check": "eslint-config-prettier src/components/app/container/index.js",
    "lint": "time npm-run-all --parallel --print-label --continue-on-error lint:*",
    "lint:css": "time stylelint src/**/*.scss",
    "lint:js": "time yarn run eslint:base __mocks__/ server/ src/ support/ webpack/ jest.config.ts postcss.config.js",
    "start": "node --experimental-json-modules server/index.mjs",
    "test": "jest",
    "types:check": "npx tsc --project tsconfig.json",
    "webpack:dev-server": "webpack serve --config=webpack/env.development.js",
    "webpack:production": "webpack --config=webpack/env.production.js",
    "webpack:stats": "yarn run webpack:production --env stats"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,mjs}": "yarn run eslint:base",
    "*.scss": "stylelint"
  },
  "dependencies": {
    "@babel/runtime": "7.14.0",
    "@reduxjs/toolkit": "1.5.0",
    "axios": "0.21.1",
    "classnames": "2.3.1",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-winston": "4.0.5",
    "lodash": "4.17.21",
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "regenerator-runtime": "0.13.5",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@babel/plugin-transform-runtime": "7.14.2",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.0-beta.8",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "autoprefixer": "9.7.6",
    "babel-jest": "27.0.2",
    "babel-loader": "8.2.2",
    "copy-webpack-plugin": "8.1.1",
    "css-loader": "5.2.4",
    "css-minimizer-webpack-plugin": "3.0.0",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "4.2.5",
    "image-minimizer-webpack-plugin": "2.2.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "9.0.0",
    "jest": "27.0.4",
    "lint-staged": "10.2.2",
    "mini-css-extract-plugin": "1.6.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.2.15",
    "postcss-loader": "5.3.0",
    "prettier": "2.3.0",
    "react-refresh": "0.10.0",
    "react-test-renderer": "17.0.1",
    "sass": "1.32.13",
    "sass-loader": "11.1.1",
    "snapshot-diff": "0.8.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.19.0",
    "ts-jest": "27.0.2",
    "ts-node": "10.0.0",
    "typescript": "4.3.2",
    "webpack": "5.37.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "4.0.0-beta.3",
    "webpack-merge": "5.7.3"
  }
}
