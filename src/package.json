{
  "name": "dishonored2-calculator",
  "version": "1.0.0",
  "description": "Power calculator for Dishonored 2.",
  "author": "Anthony Hastings <ar.hastings@gmail.com> (https://antwan1986.github.io/)",
  "license": "MIT",
  "private": true,
  "main": "scripts/index.js",
  "scripts": {
    "build": "npm run webpack:production",
    "lint": "eslint --ext .js --ext .jsx scripts/ webpack/",
    "prestart": "npm run build",
    "pretest": "npm run lint",
    "start": "node server/index.js",
    "test": "jest --config=.jestrc.json",
    "test:upload-coverage": "npm test && cat jest-coverage/lcov.info | coveralls",
    "test:watch": "npm test -- --watch",
    "webpack:dev-server": "webpack-dev-server --env.target=development --config=webpack/webpack.config.js",
    "webpack:production": "webpack --env.target=production --config=webpack/webpack.config.js --colors --progress",
    "webpack:stats": "webpack --env.target=production --env.stats=true --config=webpack/webpack.config.js --colors --progress"
  },
  "dependencies": {
    "babel-runtime": "6.23.0",
    "cors": "2.8.3",
    "express": "4.15.3",
    "immutable": "3.8.1",
    "lodash": "4.17.4",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-code-splitting": "1.1.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-immutable-proptypes": "2.1.0",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "redux": "3.7.0",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.24.1",
    "coveralls": "2.13.1",
    "enzyme": "2.8.2",
    "eslint": "4.0.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-react": "7.1.0",
    "fetch-mock": "5.11.0",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.4",
    "jest-enzyme": "3.2.0",
    "jest-immutable-matchers": "1.5.0",
    "react-test-renderer": "15.6.1",
    "redux-mock-store": "1.2.3",
    "webpack": "3.0.0",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-server": "2.5.0",
    "webpack-merge": "4.1.0"
  }
}
