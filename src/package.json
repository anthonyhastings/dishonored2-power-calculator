{
  "name": "dishonored2-calculator",
  "version": "1.0.0",
  "description": "Power calculator for Dishonored 2.",
  "author": "Anthony Hastings <ar.hastings@gmail.com> (https://antwan1986.github.io/)",
  "license": "MIT",
  "private": true,
  "main": "scripts/index.js",
  "scripts": {
    "build": "npm run webpack:production",
    "lint": "eslint --ext .js scripts/ webpack/",
    "prestart": "npm run build",
    "pretest": "npm run lint",
    "start": "node server/index.js",
    "test": "jest --config=.jestrc.json",
    "test:upload-coverage": "npm test && cat jest-coverage/lcov.info | coveralls",
    "test:watch": "npm test -- --watchAll",
    "webpack:dev-server": "webpack-dev-server --env.target=development --config=webpack/webpack.config.js",
    "webpack:production": "webpack --env.target=production --config=webpack/webpack.config.js --colors --progress",
    "webpack:stats": "webpack --env.target=production --env.stats=true --config=webpack/webpack.config.js --colors --progress"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "classnames": "2.2.5",
    "cors": "2.8.4",
    "express": "4.16.2",
    "immutable": "3.8.2",
    "lodash": "4.17.4",
    "normalize.css": "7.0.0",
    "prop-types": "15.5.10",
    "react": "16.2.0",
    "react-code-splitting": "1.2.1",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "react-immutable-proptypes": "2.1.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "7.2.4",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-jest": "22.0.4",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.17",
    "coveralls": "3.0.0",
    "css-loader": "0.28.8",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.15.0",
    "eslint-plugin-jest": "21.5.0",
    "eslint-plugin-react": "7.5.1",
    "fetch-mock": "5.13.1",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "imagemin-webpack-plugin": "2.1.5",
    "jest": "22.0.4",
    "jest-enzyme": "4.0.2",
    "node-sass": "4.7.2",
    "postcss-loader": "2.0.10",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "react-test-renderer": "16.2.0",
    "redux-mock-store": "1.4.0",
    "webmanifest-loader": "0.2.1",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.2",
    "webpack-dev-server": "2.10.0",
    "webpack-merge": "4.1.1"
  }
