{
  "name": "dishonored2-calculator",
  "version": "1.0.0",
  "description": "Power calculator for Dishonored 2.",
  "author": "Anthony Hastings <ar.hastings@gmail.com> (https://antwan1986.github.io/)",
  "license": "MIT",
  "private": true,
  "main": "scripts/index.js",
  "scripts": {
    "build": "npm run webpack:production",
    "lint": "eslint --ext .js --ext .jsx scripts/ webpack/",
    "prestart": "npm run build",
    "pretest": "npm run lint",
    "start": "node scripts/server.js",
    "test": "jest --config=.jestrc.json",
    "test:upload-coverage": "npm test && cat jest-coverage/lcov.info | coveralls",
    "test:watch": "npm test -- --watch",
    "webpack:dev-server": "webpack-dev-server --env=development --config=webpack/webpack.config.js",
    "webpack:development": "webpack --env=development --config=webpack/webpack.config.js --colors --progress",
    "webpack:production": "webpack --env=production --config=webpack/webpack.config.js --colors --progress"
  },
  "dependencies": {
    "express": "4.14.0",
    "immutable": "3.8.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-immutable-proptypes": "2.1.0",
    "react-redux": "5.0.2",
    "react-router-dom": "4.0.0",
    "redux": "3.6.0",
    "redux-immutable": "3.0.11",
    "redux-thunk": "2.2.0",
    "reselect": "2.5.4"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-env": "1.2.0",
    "babel-preset-react": "6.22.0",
    "coveralls": "2.11.16",
    "eslint": "3.14.1",
    "eslint-plugin-jest": "19.0.1",
    "eslint-plugin-react": "6.9.0",
    "html-webpack-plugin": "2.26.0",
    "jest": "19.0.1",
    "jest-immutable-matchers": "1.4.0",
    "stats-webpack-plugin": "0.4.3",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0",
    "webpack-merge": "2.4.0"
  }
}
