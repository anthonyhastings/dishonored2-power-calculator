{
  "name": "dishonored2-calculator",
  "version": "1.0.0",
  "description": "Power calculator for Dishonored 2.",
  "author": "Anthony Hastings <ar.hastings@gmail.com> (https://antwan1986.github.io/)",
  "license": "MIT",
  "private": true,
  "main": "scripts/index.js",
  "scripts": {
    "build": "npm run webpack:production",
    "lint": "eslint --ext .js scripts/ server/ webpack/",
    "pretest": "npm run lint",
    "start": "node server/index.js",
    "test": "jest",
    "test:upload-coverage": "npm test && cat jest-coverage/lcov.info | coveralls",
    "test:watch": "npm test -- --watchAll",
    "webpack:dev-server": "webpack-dev-server --env.target=development --config=webpack/webpack.config.js",
    "webpack:production": "webpack --env.target=production --config=webpack/webpack.config.js --colors --progress",
    "webpack:stats": "webpack --env.target=production --env.stats=true --config=webpack/webpack.config.js --colors --progress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "classnames": "2.2.6",
    "cors": "2.8.4",
    "express": "4.16.3",
    "immutable": "3.8.2",
    "lodash": "4.17.10",
    "normalize.css": "8.0.0",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-code-splitting": "1.2.1",
    "react-dom": "16.4.1",
    "react-hot-loader": "4.3.3",
    "react-immutable-proptypes": "2.1.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "9.0.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-jest": "22.0.4",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "coveralls": "3.0.2",
    "css-loader": "1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.2.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-react": "7.10.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.0.0-rc.13",
    "imagemin-webpack-plugin": "2.1.5",
    "jest": "23.4.1",
    "jest-enzyme": "6.0.2",
    "moxios": "0.4.0",
    "node-sass": "4.9.2",
    "postcss-loader": "2.1.6",
    "react-test-renderer": "16.4.1",
    "redux-mock-store": "1.5.3",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "webmanifest-loader": "0.3.0",
    "webpack": "4.16.2",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-merge": "4.1.3"
  }
}
